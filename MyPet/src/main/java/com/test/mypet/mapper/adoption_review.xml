<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="adoption_review">

	<select id="list" resultType="adoptionReviewDTO">
		select * from vwAdoptionReviewListVer order by seqAdoptionReview desc
	</select>
	
	<select id="get" parameterType="String" resultType="fdto">
		select * from vwFaq where seqFaq = #{seqFaq}
	</select>

</mapper>